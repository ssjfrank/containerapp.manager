{
  "ManagerSettings": {
    "PollingIntervalSeconds": 30,
    "IdleTimeoutMinutes": 10,
    "RestartVerificationTimeoutMinutes": 5,
    "RestartDelaySeconds": 5,
    "QueueContainerMappings": {
      "container-app-1": [
        "queue.app1.requests",
        "queue.app1.events"
      ],
      "container-app-2": [
        "queue.app2.requests"
      ]
    }
  },
  "EmsSettings": {
    "ServerUrl": "tcp://localhost:7222",
    "Username": "admin",
    "Password": "",
    "NotificationQueueName": "NOTIFICATION.QUEUE"
  },
  "AzureSettings": {
    "SubscriptionId": "",
    "ResourceGroupName": "",
    "UseManagedIdentity": true,
    "TenantId": "",
    "ClientId": "",
    "ClientSecret": ""
  },
  "EnableHealthChecks": false,
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "File",
        "Args": {
          "path": "logs/container-manager-.txt",
          "rollingInterval": "Day"
        }
      }
    ]
  }
}